@model IQRecruitmentTool.Models.StorageDocumentLibrary

@{
    ViewBag.Title = "Delete";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <section class="checkout">
        <div class="container">
            <div class="row">

                <div class="col-md-6 mr-auto text-left mt40 mb40">
                    <div class="section-heading">
                        <h5>Are you sure you want to delete this document?</h5>
                        <hr class="
                            ">
                    </div>
                    <dl class="dl-horizontal">
                        <dt>
                            Document Name
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.DocName)
                        </dd>

                    </dl>

                    </div>
            </div>
        </div>
        <div class="col-md-6 mr-auto text-center mt40 mb40">
            <button type="submit" value="Delete" class=" btn btn-lg btn-primary btn-square btn-appear">Delete</button>
            <div />
        </div>
    </section>
}
